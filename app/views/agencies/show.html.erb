<%= render 'layouts/header' %>
<nav class="navigation">
  <ul><%= @agency.name %></li>
<li><strong>Contact:</strong> <%=number_to_phone(@agency.phone) %></li>
        <li><strong>Address:</strong><%= @agency.address1 %><br></li>
        <% if @agency.address2.present?%><li><%=@agency.address2%></li>
        <% end %>
        <%= @agency.city %>, <%= @agency.state %> <%= @agency.zip %><br>
</nav>
        <%= link_to 'Edit', edit_agency_path(@agency), class: 'button' %>|
        <%= link_to 'Delete', @agency,
                    class: 'button',
                    method: :delete, data: { confirm: 'Are you sure?' } %>|
  <% if current_user.role == "superadmin" %>
      <a href="../agencies">View All</a>
  <% end %>

      <%= render 'layouts/footer' %>
